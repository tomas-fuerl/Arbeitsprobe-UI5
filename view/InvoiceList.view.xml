<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<List
		headerText="{i18n>invoiceListTitle}"
		class="sapUiResponsiveMargin"
		width="auto"
		items="{invoice>/Schueler}">
		<items>
			<ObjectListItem
				title="{invoice>SchülerVorname} {invoice>SchülerName}, {invoice>Abteilung}, {invoice>Klasse}, {invoice>Alter}"/>
		</items>
	</List>
</mvc:View>
